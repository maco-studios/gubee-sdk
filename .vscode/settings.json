{
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "composer.json": "composer.lock",
        "phpcs.xml": "phpcs.xml.dist,phpstan.neon,.phpcs*",
        "phpcs.xml.dist": "phpcs.xml,phpstan.neon,.phpcs*",
        ".gitignore": ".env*,.tool-versions",
        "phpunit.xml.dist": ".phpunit.*, phpunit.xml",
        "README.md": "*.md",
        "phpunit.xml": ".phpunit.*, phpunit.*"
    },
    "files.associations": {
        "*.xml.dist": "xml",
        "*.neon": "yaml"
    },
    "php.version": "8.3"
}